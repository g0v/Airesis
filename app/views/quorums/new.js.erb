$('#new_quorum_container').html("<%= escape_javascript(render(:partial => 'quorums/form', :locals => {:button_name => t('buttons.create')}))%>");

$('#new_quorum_container').dialog({
	width: 500,
	height: 'auto',
	modal : true,
	resizable: false,
	close : function(event, ui) {
		$('#new_quorum_container').dialog('destroy');
        $('#new_quorum_container').empty();
	}
});

