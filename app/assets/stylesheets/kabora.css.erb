.btn {
    text-decoration: none;
    text-shadow: 0 1px rgba(0, 0, 0, 0.1);
    font: bold 11px Arial, sans-serif;
    color: #444;
    line-height: 17px;
    display: inline-block;
    margin: 5px;
    padding: 8px 14px;
    background: #F3F3F3;
    border: solid 1px #D9D9D9;
    border-radius: 2px;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    -webkit-transition: border-color .20s;
    -moz-transition: border-color .20s;
    -o-transition: border-color .20s;
    transition: border-color .20s;
    cursor: pointer;
    position: relative;
}

.btn:hover {
    background: #f8f8f8;
    border-color: #c6c6c6;
    color: #333;
    text-decoration: none;
}

.btn:active {
    box-shadow: inset 0px 1px 2px rgba(0, 0, 0, 0.1);
}

.btn:focus {
    border: 1px solid #4D90FE;
    outline: medium none;
}

.btn:disabled, a[disabled=disabled] {
    opacity: 0.5;
}

.btn.blue {
    border: 1px solid #3079ED !important;
    background: #4d90fe;
    background: -webkit-linear-gradient(top, #4d90fe, #4787ED);
    background: -moz-linear-gradient(top, #4d90fe, #4787ED);
    background: -ms-linear-gradient(top, #4d90fe, #4787ED);
    background: -o-linear-gradient(top, #4d90fe, #4787ED);
    -webkit-transition: border .20s;
    -moz-transition: border .20s;
    -o-transition: border .20s;
    transition: border .20s;
    color: #FFF !important;
    text-shadow: none;
}

.btn.blue:hover {
    border: 1px solid #2F5BB7 !important;
    background: #3F83F1;
    background: -webkit-linear-gradient(top, #4D90FE, #357AE8);
    background: -moz-linear-gradient(top, #4D90FE, #357AE8);
    background: -ms-linear-gradient(top, #4D90FE, #357AE8);
    background: -o-linear-gradient(top, #4D90FE, #357AE8);
}

.btn.blue:active {
    border-color: #2F5BB7 !important;
    background: #357ae8;
    box-shadow: 0px 1px 2px rgba(0, 0, 0, 1);
}

.btn.green {
    color: #fff;
    border: 1px solid #29691D !important;
    background: #3D9400;
    background: -webkit-linear-gradient(top, #3D9400, #398A00);
    background: -moz-linear-gradient(top, #3D9400, #398A00);
    background: -ms-linear-gradient(top, #3D9400, #398A00);
    background: -o-linear-gradient(top, #3D9400, #398A00);
    -webkit-transition: border .20s;
    -moz-transition: border .20s;
    -o-transition: border .20s;
    transition: border .20s;
}

.btn.green:hover {
    border: 1px solid #368200 !important;
    background: #2D6200;
    background: -webkit-linear-gradient(top, #3D9400, #368200);
    background: -moz-linear-gradient(top, #3D9400, #368200);
    background: -ms-linear-gradient(top, #3D9400, #368200);
    background: -o-linear-gradient(top, #3D9400, #368200);
}

.btn.green:focus {
    border: 1px solid transparent;
    box-shadow: 0 0 0 1px #FFFFFF inset;
    outline: 0 none transparent;
}

.btn.blue:focus {
    border: 1px solid transparent;
    box-shadow: 0 0 0 1px #FFFFFF inset;
    outline: 0 none transparent;
}

.btn.red:focus {
    border: 1px solid transparent;
    box-shadow: 0 0 0 1px #FFFFFF inset;
    outline: 0 none transparent;
}

.btn.red {
    color: #fff;
    border: 1px solid #D14836 !important;
    background: #D64937;
    background: -webkit-linear-gradient(top, #DC4A38, #D14836);
    background: -moz-linear-gradient(top, #DC4A38, #D14836);
    background: -ms-linear-gradient(top, #DC4A38, #D14836);
    background: -o-linear-gradient(top, #DC4A38, #D14836);
    -webkit-transition: border .20s;
    -moz-transition: border .20s;
    -o-transition: border .20s;
    transition: border .20s;
}

.btn.red:hover {
    border: 1px solid #B0281A !important;
    background: #D14130;
    background: -webkit-linear-gradient(top, #DC4A38, #C53727);
    background: -moz-linear-gradient(top, #DC4A38, #C53727);
    background: -ms-linear-gradient(top, #DC4A38, #C53727);
    background: -o-linear-gradient(top, #DC4A38, #C53727);
}


.btn.action:hover {
    -moz-box-shadow: 0 1px 0px #DDD;
    -webkit-box-shadow: 0 1px 0px #DDD;
    box-shadow: iset 0 1px 0px #DDD;
}

.btn.action:active {
    -moz-box-shadow: none !important;
    -webkit-box-shadow: none !important;
    box-shadow: none !important;
    border-color: #C6C6C6 !important;
}

.btn.green:active {
    border-color: #2D6200 !important;
}

.btn.red:active {
    border-color: #B0281A !important;
}

