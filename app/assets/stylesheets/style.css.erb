}

.callout_sx {
    z-index: 20;
    position: absolute;
    top: 26px;
    border: 0;
    left: 280px;
}

span.tooltip_bt span.innerc {
    z-index: 10;
    display: none;
    padding: 14px 20px;
    margin-top: 40px;
    margin-left: -185px;
    margin-right: 5px;
    min-width: 200px;
    max-width: 350px;
    line-height: 16px;
}

.callout_bt {
    z-index: 20;
    position: absolute;
    border: 0;
    top: -12px;
    left: 160px;
}

span.tooltip_tp span.innerc {
    z-index: 10;
    display: none;
    padding: 14px 20px;
    margin-top: -110px;
    margin-left: -145px;
    width: 240px;
    line-height: 16px;
    height: 63px;
}

.callout_tp {
    z-index: 20;
    position: absolute;
    border: 0;
    top: 90px;
    left: 120px;
}

span.tooltip_sx strong, span.tooltip_bt strong, span.tooltip_tp strong {
    line-height: 30px;
}

span.tooltip, span.tooltip_sx, span.tooltip_bt, span.tooltip_tp {
    outline: none;
}

span.tooltip_sx:hover, span.tooltip_bt:hover, span.tooltip_tp:hover {
    text-decoration: none;
}

