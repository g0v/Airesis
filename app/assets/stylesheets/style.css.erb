}

span.tooltip:hover span.innerc {
    display: inline;
    position: absolute;
    color: #111;
    border: 1px solid #99CC00;
    background: #E2F0B6 url(/assets/css-tooltip-gradient-bg.png) repeat-x 0 0;
}

.callout {
    z-index: 20;
